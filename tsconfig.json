{
  "compilerOptions": {
    "declaration": true,
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "module": "commonjs",
    "target": "es5",
    "outDir": "dist",
    "lib": [
      "es6",
      "dom"
    ],
    "types": [
      "mocha",
      "node",
      "chai-subset"
    ]
  },
  "include": [
    "src"
  ]
}
